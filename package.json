    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.11.6",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.4.1",
    "@types/koa-subdomain": "^3.0.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.10",
    "@types/pg": "^8.6.5",
    "@types/pump": "^1.1.1",
