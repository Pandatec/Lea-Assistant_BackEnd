{
  "name": "lea-back",
  "version": "1.0.0",
  "description": "Lea Project back-end",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc && node dist/src/app.js",
    "start": "nodemon --config nodemon.json src/app.ts",
    "dummy_patient": "node -r ts-node/register ./utils/dummy_patient.ts",
    "test_load": "node test/load/client.js",
    "test": "nyc --reporter=text mocha --timeout 30000 -r ts-node/register ./test/**/*.ts --require ./test/hooks.ts --exit --bail",
    "new_migration": "node -r ts-node/register ./src/new_migration.ts",
    "migrate": "node -r ts-node/register ./src/migrate.ts",
    "transition": "node -r ts-node/register ./src/transition.ts",
    "create_db": "node -r ts-node/register ./src/create_db.ts",
    "drop_db": "node -r ts-node/register ./src/drop_db.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lea-Voc/Lea-Back.git"
  },
  "author": "",
